<div class="row">
    <div class="col-xs-12">
        <ul class="breadcrumb text-ellipsis">
            {% for label, link in links %}
                <li class="breadcrumb-item">
                    {% if loop.last %}
                        <a href="{{ link }}"><strong>{{ label }}</strong></a>
                    {% else %}
                        <a href="{{ link }}">{{ label }}</a>
                    {% endif %}
                </li>
            {% endfor %}
        </ul>
    </div>
</div>